<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>EMS</display-name>
  <servlet>
    <description>JAX-RS Tools Generated - Do not modify</description>
    <servlet-name>JAX-RS Servlet</servlet-name>
    <servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>action.Login</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GetRepairLists</servlet-name>
    <servlet-class>action.GetRepairLists</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FirstCheck</servlet-name>
    <servlet-class>action.FirstCheck</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Distribute</servlet-name>
    <servlet-class>action.Distribute</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>JAX-RS Servlet</servlet-name>
    <url-pattern>/jaxrs/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/EMS/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetRepairLists</servlet-name>
    <url-pattern>/EMS/GetRepairLists</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FirstCheck</servlet-name>
    <url-pattern>/EMS/FirstCheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Distribute</servlet-name>
    <url-pattern>/EMS/Distribute</url-pattern>
  </servlet-mapping>
</web-app>